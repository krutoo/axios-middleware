{
  "name": "middleware-axios",
  "version": "1.0.0",
  "description": "Axios with express/koa like middleware",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:badges": "npm run test:coverage && jest-coverage-badges --output './badges'",
    "start:example": "parcel ./example/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krutoo/middleware-axios.git"
  },
  "keywords": [
    "axios",
    "fetch",
    "api",
    "javascript"
  ],
  "author": "Dmitry Petrov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krutoo/middleware-axios/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test:badges"
    }
  },
  "homepage": "https://github.com/krutoo/middleware-axios#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/jest": "^24.0.18",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "eslint": "^6.2.1",
    "eslint-plugin-jest": "^22.15.2",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "jest-coverage-badges": "^1.1.2",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "lodash": "^4.17.15"
  }
}

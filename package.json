{
  "name": "middleware-axios",
  "version": "0.0.0",
  "description": "Axios with express/koa like middleware",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint ./src",
    "test": "jest",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "type-check": "tsc -p . --noEmit",
    "example:start": "cd example && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krutoo/middleware-axios.git"
  },
  "keywords": [
    "axios",
    "fetch",
    "api",
    "javascript",
    "middleware"
  ],
  "author": "Dmitry Petrov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krutoo/middleware-axios/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test:coverage"
    }
  },
  "homepage": "https://github.com/krutoo/middleware-axios#readme",
  "peerDependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "axios": "^0.21.1",
    "babel-jest": "^27.1.0",
    "eslint": "^7.27.0",
    "jest": "^27.1.0",
    "msw": "^0.35.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.3"
  }
}

{
  "name": "middleware-axios",
  "version": "0.0.0",
  "description": "Axios with express/koa like middleware",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint ./src",
    "build": "rimraf dist && tsc -p .",
    "type-check": "tsc -p . --noEmit",
    "example:start": "cd example && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krutoo/middleware-axios.git"
  },
  "keywords": [
    "axios",
    "fetch",
    "api",
    "javascript",
    "middleware"
  ],
  "author": "Dmitry Petrov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krutoo/middleware-axios/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test:coverage"
    }
  },
  "homepage": "https://github.com/krutoo/middleware-axios#readme",
  "peerDependencies": {
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "axios": "0.24.0",
    "eslint": "^8.4.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  }
}
